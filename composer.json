{
  "name": "tmeister/better-social-wp",
  "description": "Just a proof-of-concept plugin to create better social images automatically.",
  "homepage": "https://github.com/Tmeister/better-social-wp/",
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Enrique Chavez",
      "homepage": "https://enriquechavez.co"
    }
  ],
  "support": {
    "issues": "https://github.com/Tmeister/better-social-wp/issues",
    "source": "https://github.com/Tmeister/better-social-wp/"
  },
  "config": {
    "vendor-dir": "includes/vendor",
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": ">=7.4",
    "composer/installers": "~1.0",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "*"
  }
}
